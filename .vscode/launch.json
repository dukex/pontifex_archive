  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run",
      "type": "dart",
      "request": "launch",
      "cwd": "${workspaceFolder}/app",
      "program": "lib/main.dart"
    },
    {
      "type": "debugpy",
      "request": "launch",
      "module": "poetry",
      "name": "Generate",
      "cwd": "${workspaceFolder}/epub_builder",
      "args": [
        "run",
        "generate"
      ],
      "console": "integratedTerminal"
    }
  ]
}